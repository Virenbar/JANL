{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "group:allNonMajor",
    ":semanticCommitTypeAll(chore)",
    "schedule:quarterly"
  ],
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "description": "Skip nuget",
      "matchManagers": [ "nuget" ],
      "enabled": false
    }
  ]
}